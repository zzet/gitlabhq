- user         = event.author
- service      = event.source
- project      = event.target

= raw "User #{ link_to user.name, user_url(user.username)} added '#{ service.title }' service to #{ link_to project.path_with_namespace, project_url(project.path_with_namespace)} project:"
%table
  %tr
    %td Name
    %td= service.title
  %tr
    %td Type
    %td= service.type
%br
